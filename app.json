{
    "expo": {
      "name": "Gigs",
      "slug": "gigs",
      "version": "1.0.0",
      "entryPoint": "index.js",
      "orientation": "portrait",
      "icon": "./assets/images/GIGS_logo.png",
      "splash": {
        "image": "./assets/images/GIGS_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
        "bundleIdentifier": "com.systemaddict.gigs",
        "supportsTablet": true
      },
      "android": {
        "package": "com.systemaddict.gigs",
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/GIGS_logo.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/images/GIGS_logo.png"
      }
    }
  }
  